        // -------------------------------------------------------------------
        function applicationLogOut() {
            swal({
                title: "Daily Report Notification",
                text: "Send To Email Address",
                //icon: "info", dangerMode: true,
                content: 'input',
                closeOnClickOutside: false, closeOnEsc: false,
                buttons: {
                    cancel: { text: 'No, Please LogOut', className: 'gradient-45deg-yellow-green', visible: true },
                    confirm: { text: 'Yes, Please', className: 'gradient-45deg-purple-deep-orange', visible: true }
                }
            }).then(function (isConfirm) {
                if (isConfirm) {
                    debugger;
                    var tmpEmail = $('.swal-content__input').val();
                    jQuery.ajax({
                        type: "POST",
                        contentType: "application/json; charset=utf-8",
                        url: "Services/DashboardService.asmx/SendDailyReport",
                        data: '{ToEmailAddress:\'' + tmpEmail + '\'}',
                        success: function (response) {
                            debugger;
                            swal({ title: "Message", text: response.d, icon: 'success' });
                        },
                        error: function (r) { alert('Error : ' + r.responseText); },
                        failure: function (r) { alert('failure'); }
                    });
                }
                else {
                    window.location.href = "../Default.aspx";
                }
            });
        }
        // *-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*
        // Section : DailySummary 
        // *-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*
        $('.sidenav-close').click(function () {
