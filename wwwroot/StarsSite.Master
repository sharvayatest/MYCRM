                    return jQuery('#Sharvayacontent').removeClass('hide').addClass('popupsticky');
                else
                    return jQuery('#Sharvayacontent').removeClass('popupsticky').addClass('hide');
            });
            jQuery(document).on('click', function (e) {
                //alert('doc clicked')
