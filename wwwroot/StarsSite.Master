            $('#ifrDivSummary').html('');
        })

        function openDashboardDailySummary() {
            //alert('open summary')
            var current_pull = parseInt($('#theme-cutomizer-out-summary').css('transform').split(',')[5]);
            $('#theme-cutomizer-out-summary').css({ 'transform': 'translateX(0%)' });
            if (!$('#ifrModuleDailyNew').length) {
                $('#ifrDivSummary').html('<iframe id="ifrModuleDailyNew" src="DashboardDailySummary.aspx" runat="server" clientidmode="static" style="overflow-x:hidden; z-index: 999; top: 70px; width: 100%; height:800px;"></iframe>');
