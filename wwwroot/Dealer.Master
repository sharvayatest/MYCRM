        /*--------------------------------------------------------------*/
        .navbox {
            background-color: white;
            width: 1330px;
            height: 525px;
            padding: 5px;
            -webkit-backface-visibility: initial;
            backface-visibility: initial;
            border: none;
            border-radius: 5px;
            z-index:0;
        }
        /*--------------------------------------------------------------*/
        .navbox-tiles {
            width: 120px;
            height:70px;
            min-height:70px;
            padding: 5px;
            margin:5px;
            float:left;
            border: none;
            border-radius: 8px;
            background-color: transparent;
        }

        .navbox-tiles .tile {
            display: block;
            width: 100%;
            height: 0;
            padding-bottom:10px;
            float: left;
            border: 2px solid transparent;
            color: navy;
            border-radius: 8px;
            position: relative;
        }

        .navbox-tiles .tile .icon {
            width: 100%;
            height: 40px;
            text-align: center;
            position: relative;
            bottom:0px;
            background-color: transparent;
        }

        .navbox-tiles .tile .icon .fa {
            font-size: 35px;
            position: absolute;
            top: 50%;
            left: 50%;
            -webkit-transform: translate(-50%, -50%);
            -ms-transform: translate(-50%, -50%);
            transform: translate(-50%, -50%);
            -webkit-backface-visibility: initial;
            backface-visibility: initial;
        }

        .navbox-tiles .tile .title {
            width:100%;
            padding: 5px;
            font-size: 12px;
            position: relative;
            bottom: 0;
            text-align:center;
        }

        .navbox-tiles .tile:hover {
            border-color: #fff;
            text-decoration: none;
        }

        .navbox-tiles .tile:not(:nth-child(3n+3)) {
            margin-right: 4.54545454545455%;
        }

        .navbox-tiles .tile:nth-child(n+4) {
            margin-top: 15px;
        }

        @media screen and (max-width: 370px) {

