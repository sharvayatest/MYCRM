        function showReminderMsg() {
            var x = window.localStorage.getItem("LastReminderStamp");
            if (x != null && x != "") {
                var laststamp = new Date(window.localStorage.getItem("LastReminderStamp"));
                var currstamp = new Date();
                var diffmili = Math.abs(laststamp - currstamp);
                var minutes = Math.floor(diffmili / 1000) / 60;
                //alert(laststamp)
                //alert(currstamp)
                //alert(diffmili)
                //alert(minutes)
                if (minutes > 2) {
                    //alert(minutes)
                    //alert(window.localStorage.getItem("LastReminderStamp"))
                    window.localStorage.setItem("LastReminderStamp", new Date());
                    jQuery.ajax({
                        type: "POST",
                        contentType: "application/json; charset=utf-8",
                        url: "Services/NagrikService.asmx/GetReminderList",
                        dataType: "json",
                        success: function (response) {
                            //debugger;
                            var dataSet = JSON.parse(response.d);
                            var msgString = "";
                            for (i = 0; i < dataSet.length; i++) {
                                var rowdata = dataSet[i];
                                msgString += "<li class='timeline-items timeline-icon-red active'>";
                                msgString += "<div class='timeline-time'>" + rowdata.CreatedDate + "</div>";
                                msgString += "<h6 class='timeline-title'>" + rowdata.ModuleName + "</h6>";
                                msgString += "<p class='timeline-text'>" + rowdata.Description + "</p>";
                                msgString += "</li>";
                            }
                            if (msgString.length > 0) {
                                $("#ulintroReminder").append(msgString);
                                const elem = document.getElementById('intromodal');
                                const instance = M.Modal.init(elem, { dismissible: false });
                                instance.open();
                            }

                        },
                        error: function (r) { alert('Error : ' + r.responseText); },
                        failure: function (r) { alert('failure'); }
                    });
                }
            }
            else {
                window.localStorage.setItem("LastReminderStamp", new Date());
            }
        }

