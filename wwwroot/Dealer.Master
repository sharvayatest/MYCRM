        function buildBottomMenu(items) {
            jQuery.each(items, function () {
                var strID = '#mpanel' + this.pkID;
                var currID = jQuery(strID);
                currID.empty();
                if (this.ParentId == null) {
                    if (this.List && this.List.length > 0) {
                        jQuery.each(this.List, function () {
                            var tmpStr = "";
                            tmpStr += '<div id="' + this.MenuName + '" class="navbox-tiles" style="border-radius:8px;">';
                            tmpStr += '<a href="' + this.MenuURL + '" class="tile">';
                            tmpStr += '<div class="icon">';
                            tmpStr += '<img alt="" src="' + this.MenuImage + '" height="' + this.MenuImageHeight + '" width="' + this.MenuImageWidth + '" title="Dashboard" />';
                            tmpStr += '</div>';
                            tmpStr += '<span class="title">' + this.MenuText + '</span>';
                            tmpStr += '</a>';
                            tmpStr += '</div>';
                            jQuery(tmpStr).appendTo(currID);
                        });
                    }
                }

            });
        }

        // ------------------------------------------------------------
        function buildMenu(parent, items) {
            var cnt = 1;
            //debugger;
            jQuery.each(items, function () {
                var li = "";
                if (this.MenuLevel == 1) {
                    var tmpStr = "";
                    //alert(this.MenuName)
                    if (this.MenuURL == "" || this.MenuURL == null) {
                        tmpStr = "<li id='" + this.MenuName + "' class='bold'>";
                        tmpStr += "<a class='collapsible-header waves-effect waves-cyan' href='JavaScript:void(0)'>";
                        tmpStr += "<i class='material-icons'>settings_input_svideo</i>";
                        tmpStr += "<span class='menu-title' data-i18n='" + this.MenuText + "'>" + this.MenuText + "</span>";
                        tmpStr += "</a></li>";
                    }
                    else {

                        if (this.MenuLevel == 1 && cnt == 1)
                            tmpStr = "<li id='" + this.MenuName + "' class='active bold'>";
                        else
                            tmpStr = "<li id='" + this.MenuName + "' class='bold'>";
                        tmpStr += "<a class='active' href='#'>";
                        tmpStr += "<i class='material-icons'>settings_input_svideo</i>";
                        tmpStr += "<span class='menu-title' data-i18n='" + this.MenuText + "'>" + this.MenuText + "</span>";
                        tmpStr += "</a></li>";
                    }
                    li = jQuery(tmpStr);
                }
                else {
                    var tmpStr = "", tmpLevel = "";
                    //alert(this.MenuName)
                    /* --------------------------------------------------------------------- */
                    if (this.MenuURL == "" || this.MenuURL == null) {

                        if (this.MenuLevel == 2) {
                            tmpStr = "<li id='" + this.MenuName + "' style='margin-left:7px;'>";
                            tmpStr += "<a class='collapsible-header waves-effect waves-cyan' href='JavaScript:void(0)'>";
                            tmpStr += "<i class='material-icons'>radio_button_unchecked</i><span data-i18n='" + this.MenuText + "'>" + this.MenuText + "</span>";
                            tmpStr += "</a>";
                            tmpStr += "</li>";
                        }
                        else if (this.MenuLevel == 3) {
                            tmpStr = "<li id='" + this.MenuName + "' style='margin-left:15px;'>";
                            tmpStr += "<a class='collapsible-header waves-effect waves-cyan' href='JavaScript:void(0)'>";
                            tmpStr += "<i class='material-icons'>radio_button_unchecked</i><span data-i18n='" + this.MenuText + "'>" + this.MenuText + "</span>";
                            tmpStr += "</a>";
                            tmpStr += "</li>";
                        }
                        else if (this.MenuLevel == 3) {
                            tmpStr = "<li id='" + this.MenuName + "' style='margin-left:15px;'>";
                            tmpStr += "<a class='collapsible-header waves-effect waves-cyan' href='JavaScript:void(0)'>";
                            tmpStr += "<i class='material-icons'>radio_button_unchecked</i><span data-i18n='" + this.MenuText + "'>" + this.MenuText + "</span>";
                            tmpStr += "</a>";
                            tmpStr += "</li>";
                        }
                    }
                    else {
                        if (this.MenuLevel == 2) {
                            tmpStr = "<li id='" + this.MenuName + "'>";
                            tmpStr += "<a href='" + this.MenuURL + "'>";
                            tmpStr += "<i class='material-icons'>lens</i><span data-i18n='" + this.MenuText + "'>" + this.MenuText + "</span>";
                            tmpStr += "</a>";
                            tmpStr += "</li>";
                        }
                        else if (this.MenuLevel == 3) {
                            tmpStr = "<li id='" + this.MenuName + "'>";
                            tmpStr += "<a href='" + this.MenuURL + "'>";
                            tmpStr += "<i class='material-icons'>lens</i><span data-i18n='" + this.MenuText + "'>" + this.MenuText + "</span>";
                            tmpStr += "</a>";
                            tmpStr += "</li>";
                        }
                        else if (this.MenuLevel == 4) {
                            tmpStr = "<li id='" + this.MenuName + "'>";
                            tmpStr += "<a href='" + this.MenuURL + "'>";
                            tmpStr += "<i class='material-icons'>lens</i><span data-i18n='" + this.MenuText + "'>" + this.MenuText + "</span>";
                            tmpStr += "</a>";
                            tmpStr += "</li>";
                        }

                    }
                    li = jQuery(tmpStr);
                }
                // ------------------------------------------------------
                jQuery(li).appendTo(parent);
                // ------------------------------------------------------
                if (this.List && this.List.length > 0) {
                    var dd = jQuery("<div class='collapsible-body'></div>");
                    var ul;
                    if (this.MenuLevel == 2)
                        ul = jQuery("<ul class='collapsible' data-collapsible='accordion'></ul>");
                    else
                        ul = jQuery("<ul class='collapsible collapsible-sub' data-collapsible='accordion'></ul>");
                    ul.appendTo(dd.appendTo(li));
                    buildMenu(ul, this.List);
                }
                else {

                }
            });
        }
